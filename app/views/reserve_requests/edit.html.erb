<h1>Editar Solicitud de Reserva de <%= @publication.title %><h1>

<%= form_with model: [@publication, @reserve_request] do |form| %>
    <div>
        <%= form.label "Inicio Reserva" %>
        <%= form.datetime_local_field :start_date %>
